{
    "sync": {
        "required uInt32 type" : 1,
        "required uInt32 ballPosPlayerId" : 2,
        "required uInt32 timeStamp" : 3,
        "repeated float teamPos" : 4
    },

    "triggerMenu" : {
        "required int32 menuType" : 1,
        "repeated int32 attackPlayers" : 2,
        "repeated int32 defendplayers" : 3
    },

    "instructions" : {
        "message InsStructure" : {
            "required int32 side" : 1,
            "required int32 playerNumber" : 2,
            "required int32 ins" : 3
        },

        "repeated InsStructure instructions" : 1,
        "required int32 result" : 2,
        "required int32 ballSide" : 3,
        "required int32 playerNumber" : 4,
        "required float ballPosX" : 5,
        "required float ballPosY" : 6
    },

    "startMatch" : {
        "message Player" : {
            "required int32 uid" : 1,
            "repeated float teamPos" : 2
        },

        "required Player left" : 1,
        "required Player right" : 2,
        "required int32 kickOffSide" : 3,
        "required uInt32 startTime" : 4
    },

    "match.matchHandler.menuCmd" : {
        "required uInt32 countDown" : 1
    }
}